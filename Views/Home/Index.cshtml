@model IEnumerable<Identity.Models.ApplicationUser>

@{
    ViewData["Title"] = "Home Page";
}

<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>First Name</th>
        <th>Last Name</th>
        <th>
            Email
        </th>
    </tr>
    @foreach (var item in Model) { 
<tr>
    <td>@item.Id</td>
    <td>@item.FirstName</td>
    <td>@item.LastName</td>
    <td>@item.Email</td>
    <td>
        <a asp-action="EditUser" asp-controller="Home" asp-route-id="@item.Id">Edit</a> |
        <a asp-action="EditUserRole" asp-controller="Home" asp-route-id="@item.Id">Edit User Role</a>
    </td>
</tr>
    }
</table>